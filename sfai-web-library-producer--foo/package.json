{
  "name": "sfai-web-module-producer--foo",
  "private": true,
  "version": "1.0.3",
  "description": "SFAI Web Module Producer Template",
  "type": "module",
  "workspaces": [
    "using_app"
  ],
  "engines": {
    "node": "18.20.3",
    "npm": "9.4.1"
  },
  "scripts": {
    "clean": "rm -rf node_modules && npm cache clean --force",
    "clean-cache": "rm -rf node_modules/.cache && rm -rf .cache && rm -rf dist",
    "lint": "eslint --fix \"**/*.{js,jsx,ts,tsx}\"",
    "build:ts": "tsc",
    "build:dev:usingApp": "npm run clean-cache && npm run build:ts && cross-env ENTRY=using_app SFAI_APP_ENV=dev webpack --config config/webpack/webpack.dev.mjs",
    "start:dev:usingApp": "npm run build:ts && cross-env ENTRY=using_app SFAI_APP_ENV=dev webpack serve --config config/webpack/webpack.dev.mjs",
    "build:qa:usingApp": "npm run clean-cache && npm run build:ts && cross-env ENTRY=using_app SFAI_APP_ENV=qa webpack --config config/webpack/webpack.qa.mjs",
    "start:qa:usingApp": "npm run build:ts && cross-env ENTRY=using_app SFAI_APP_ENV=qa webpack serve --config config/webpack/webpack.qa.mjs",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:unit:dev": "echo 'skipping unit tests'",
    "test:integration:dev": "echo 'skipping integration tests'"
  },
  "keywords": [
    "react",
    "component",
    "library"
  ],
